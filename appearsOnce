public static int singleNumber(int[] A) {
    
    HashMap<Integer, Integer> map = new HashMap<>();

    for (int number : A) {
        if (map.containsKey(number)) {
            map.put(number, map.get(number) +1);
        } else {
            map.put(number, 1);
        }
    }
    
    for(Map.Entry<Integer, Integer> entry : map.entrySet()) { 
        if (entry.getValue() == 1) {
            return entry.getKey();
        }
    } 
    
    return -1;
}
